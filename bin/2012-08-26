#!/usr/bin/env zsh

DATE=$(basename $0)
MP3NUM=DR000129
FNAME=Heb-10-Embrace-Christian-Community
SRC=/Users/aar/Documents/Personal/Trinity/audio/_SOURCES/$DATE/$MP3NUM.mp3
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2012
COMPOSER="Matt McCullough"
ALBUM="Book of Hebrews"
GENRE=Sermon
COMMENT="Christian community is the key to holding on to faith and to living for the return of Jesus. This passage helps clarify what Christian community is and how it works."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
