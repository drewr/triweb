#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-8-True-Freedom
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="Jesus offers freedom to everyone who hears his word and embraces him.  But in what sense do we need freedom?  How do we know Jesus can deliver?  And how is it that he can set us free?"

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
