#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-6-What-It-Means-To-Follow-Jesus
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Bill Heerman"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="Jesus came to save people from their biggest problem: death."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
