#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=1-Cor-1-The-Foolishness-And-Glory-Of-God
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="Matt McCullough"
ALBUM="1 Corinthians: The Cross-Centered Community"
GENRE=Sermon
COMMENT="Paul calls the message of the cross foolishness, and there's no question that's how it's always seemed to the powers that be.  But what did Paul mean by this description?  And why would God choose this foolishness?"

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
