#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-12-For-This-Purpose
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="Jesus came to win glory---the glory of his death, the glory of his Father, and the glory of all those who trust in him."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
