#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=Jn-1-Child-Of-God
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="Matt McCullough"
ALBUM="Sunday Sermons"
GENRE=Sermon
COMMENT="God became a child to make you a child of God."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
