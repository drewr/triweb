#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=1-Cor-1-Be-United
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="Matt McCullough"
ALBUM="1 Corinthians: The Cross-Centered Community"
GENRE=Sermon
COMMENT="In this passage Paul confronts one of the major problems in the church at Corinth and in so many churches since then: the people were divided against each other. Paul points us to where division comes from, why it's so dangerous, and how the cross helps us overcome it."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
