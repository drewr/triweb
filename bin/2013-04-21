#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=Is-65-New-Creation
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="Matt McCullough"
ALBUM="The Gospel According To Isaiah"
GENRE=Sermon
COMMENT="God's plan to redeem extends as far as the brokenness of the world. In the new heavens and the new earth, there will be gladness and no sorrow; life and no death; fruitfulness and no futility; peace and no hostility."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
