#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-8-See-The-Light
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="The cross of Jesus is the ultimate light through which we understand the world, find hope in sorrow, and experience freedom from sin."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
