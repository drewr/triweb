#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=1-Cor-5-Love-Judgment-Healthy-Church
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="Matt McCullough"
ALBUM="1 Corinthians: The Cross-Centered Community"
GENRE=Sermon
COMMENT="Could it ever be a good thing to remove someone from the church because of their sin? Paul's stern warning explains why we must confront sin if we truly love each other, if we truly love the church body, and if we truly love God and his glory."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
