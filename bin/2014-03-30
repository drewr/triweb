#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-4-Call-To-Worship
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="Jesus' conversation with the Samaritan woman clarifies the central calling of our lives.  We're called to be worshipers, and we're called to seek worshipers."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
