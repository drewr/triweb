#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=1-Cor-14-When-We-Gather
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="Matt McCullough"
ALBUM="1 Corinthians: The Cross-Centered Community"
GENRE=Sermon
COMMENT="The church comes together not for private communion with God but to encourage each other with the gospel. We encourage each other by using our minds to engage our hearts."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
