#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-17-Jesus-Prays
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Drew Raines"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="Jesus ends his time with his disciples in prayer.  What requests does he make to His Father and what relevance do they have to us?"

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
