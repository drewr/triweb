#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-13-He-Loved-Them-To-The-End
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="Jesus serves us, so we serve each other."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
