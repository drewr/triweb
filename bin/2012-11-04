#!/usr/bin/env zsh

DATE=$(basename $0)
MP3NUM=DR000145
FNAME=Heb-12-Gods-Kingdom
SRC=/Users/aar/Documents/Personal/Trinity/audio/_SOURCES/$DATE/$MP3NUM.mp3
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2012
COMPOSER="Matt McCullough"
ALBUM="Book of Hebrews"
GENRE=Sermon
COMMENT="This passage, the rhetorical climax of the letter, summarizes the major themes of Hebrews.  It reminds us where we've come through Jesus, where we're going, and how we get there."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
