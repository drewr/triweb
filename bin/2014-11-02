#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-15-Chosen-To-Go
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="If you're a follower of Jesus, you have been chosen by God to live your life on his mission with the promise of his power."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
