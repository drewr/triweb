#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-6-Feeding-On-Jesus
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="Jesus uses vivid imagery as a metaphor for belief. He is the bread of life and the living water, and whoever feeds on his flesh and drinks his blood has eternal life."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
