#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=1-John-3-The-Power-Of-Biblical-Hope
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Bill Maynard"
ALBUM="Sunday Sermons"
GENRE=Sermon
COMMENT="We have a hope that does not disappoint, one in which we should invest everything."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
