#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-13-A-New-Commandment
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="The primary mark of a genuine follower of Jesus is a love for others that reflects Jesus's love for us.  So what does this love look like?"

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
