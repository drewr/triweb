#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=John-16-How-The-Spirit-Helps-Us
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2014
COMPOSER="Matt McCullough"
ALBUM="The Gospel Of John"
GENRE=Sermon
COMMENT="The Spirit helps us see ourselves for who we really are--our sin, our self-righteousness, our misjudgments about what is good--and the Spirit helps us see Jesus for the all-satisfying glory that he is."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
