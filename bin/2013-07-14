#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=Gal-2-Crucified-With-Christ
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="David Atchison"
ALBUM="Sunday Sermons"
GENRE=Sermon
COMMENT="When we add to the gospel, we nullify it."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
