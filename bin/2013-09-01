#!/usr/bin/env zsh

SRC=$1
DATE=$(basename $0)
FNAME=1-Cor-7-There-Remain-With-God
TITLE=$DATE-$FNAME
AUTHOR="Trinity Church"
YEAR=2013
COMPOSER="Matt McCullough"
ALBUM="1 Corinthians: The Cross-Centered Community"
GENRE=Sermon
COMMENT="How can we be free from restlessness and discontent? Paul helps us connect with God's acceptance of us, his sovereignty over us, and his future for us."

export SRC TITLE AUTHOR YEAR COMPOSER ALBUM GENRE COMMENT

./encode
